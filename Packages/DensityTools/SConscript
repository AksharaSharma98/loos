#!/usr/bin/env python

import sys
Import('env')

apps = 'blobid blob_stats grid2xplor gridstat pick_blob gridmask contained'
apps += ' gridslice gridtst grid2ascii peakify gridgauss-old gridinfo'
apps += ' gridscale'


gridtools = []

for name in Split(apps):
    fname = name + '.cpp'
    prog = env.Program(fname)
    gridtools.append(prog)



gridgauss = env.Program("gridgauss", ["gridgauss.cpp", "#/banal-lib.cpp"])
gridtools.append(gridgauss)




Return('gridtools')
