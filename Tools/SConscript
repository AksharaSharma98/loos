import sys

Import('env')

apps = 'density-dist density-dist-windowed center-pdb pdb2matlab dcdframe2pdb contacts order_params bounding aligner svd rmsds xy_rdf xy_rdf_timeseries pdbselect rdf atomic-rdf crossing-waters'
apps = apps + ' svdcolmap averager amber2pdb amber2dcd'

list = []

for name in Split(apps):
    fname = name + '.cpp'
    prog = env.Program(fname)
    list.append(prog)

Return('list')
