Import('env')



test_apps = 'atomic-group-test parser-test timeseries-test find_perf_test pdb-tests alignment-tests'
list = []

for name in Split(test_apps):
    fname = name + '.cpp'
    prog = env.Program(fname)
    list.append(prog)

Return('list')
